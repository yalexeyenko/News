<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.epam.yalexeyenko.model.News" table="NEWSBLOCK">
		<id name="id">
			<column name="ID" />
			<generator class="increment" />
		</id>
		<property name="newsTitle">
			<column name="TITLE" />
		</property>
		<property name="date">
			<column name="NEWSDATE" />
		</property>
		<property name="brief">
			<column name="BRIEF" />
		</property>
		<property name="content">
			<column name="NEWSCONTENT" />
		</property>
	</class>	
</hibernate-mapping>